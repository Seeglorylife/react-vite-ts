{
	"name": "react-vite-ts",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "VITE_CJS_TRACE=true && vite dev",
		"build:dev": "tsc && vite build --mode dev",
		"build:test": "tsc && vite build --mode test",
		"build:prod": "tsc && vite build --mode prod",
		"lint": "prettier --check 'src/**/*.{ts,tsx,js,jsx}' && eslint 'src/**/*.{ts,tsx,js,jsx} --fix'",
		"preview": "vite preview",
		"format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,less,scss,html,md}'"
	},
	"dependencies": {
		"antd": "^5.17.0",
		"axios": "^1.6.8",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.11",
		"i18next": "^23.11.4",
		"i18next-browser-languagedetector": "^7.2.1",
		"jsencrypt": "^3.3.2",
		"lodash": "^4.17.21",
		"nprogress": "^0.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^14.1.1",
		"react-router-dom": "^6.23.0",
		"zustand": "^4.5.2",
		"unocss": "^0.59.4"
	},
	"devDependencies": {
		"@types/crypto-js": "^4.2.2",
		"@types/lodash": "^4.17.1",
		"@types/node": "^20.12.5",
		"@types/nprogress": "^0.2.3",
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"@vitejs/plugin-legacy": "^5.4.0",
		"@vitejs/plugin-react": "^4.2.1",
		"@iconify/json": "^2.2.205",
		"@unocss/eslint-config": "^0.59.4",
		"@unocss/preset-attributify": "^0.59.4",
		"@unocss/preset-icons": "^0.59.4",
		"@unocss/preset-rem-to-px": "^0.59.4",
		"@unocss/preset-wind": "^0.59.4",
		"@unocss/reset": "^0.59.4",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"unplugin-icons": "^0.18.5",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"typescript": "^5.2.2",
		"vite": "^5.2.0"
	}
}
